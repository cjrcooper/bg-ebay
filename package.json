{
  "name": "bg-ebay",
  "version": "1.0.0",
  "description": "A simple ebay app to search and out ebay searches",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/cjrcooper/bg-ebay",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "appId": "1234",
    "mac": {
      "target": [
        "zip"
      ],
      "category": "your.app.category.type",
      "icon": "ebay_icon.icns"
    },
    "icon": "ebay_512.png"
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^20.8.1",
    "nodemon": "^1.10.2"
  },
  "dependencies": {
    "ebay-api": "^1.13.7",
    "electron-handlebars": "^2.0.0",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-reload": "^1.2.2",
    "electron-updater": "^2.21.3",
    "excel4node": "^1.3.5",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "lodash": "^4.17.5",
    "nodemailer": "^4.6.3",
    "opn": "^5.3.0"
  }
}
